{
	"suggest.noselect": false,
	"suggest.timeout": 2000,
	"suggest.snippetIndicator": "*",
	"diagnostic.virtualText": true,
	"python.setLinter": "PyLint",
	"python.setInterpreter": "/usr/bin/python3",
	"rust-analyzer.serverPath":"/home/uuu/.local/bin/rust-analyzer",

"languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  }
}
